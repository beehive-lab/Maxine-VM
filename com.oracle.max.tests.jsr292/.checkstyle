<?xml version="1.0" encoding="UTF-8"?>
<fileset-config file-format-version="1.2.0" simple-config="true">
	<local-check-config name="Checks" location="/com.oracle.max.base/.checkstyle_checks.xml" type="project" description="">
		<additional-data name="protect-config-file" value="false"/>
	</local-check-config>
	<fileset name="all" enabled="true" check-config-name="Checks" local="true">
		<file-match-pattern match-pattern="." include-pattern="true"/>
	</fileset>
	<filter name="FileTypesFilter" enabled="true">
		<filter-data value="java"/>
	</filter>
</fileset-config>
